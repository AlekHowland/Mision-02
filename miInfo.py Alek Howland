matricula = str(input("Tu matricula:"))
carrera = str(input("Tu carrera:"))
prepa = str(input("Escuela de procedencia:"))
descrip = str(input("Dos o tres lineas que te describan de manera general:"))

print ("."*20)

print ("Nombre:",nombre)
print ("."*20)
print ("Matricula:",matricula)
print ("."*20)
print ("Carrera:", carrera)
print ("."*20)
print ("Escuela de procedencia:", prepa)
print ("."*20)
print ("Descripcion:", descrip)
print ("."*20)

